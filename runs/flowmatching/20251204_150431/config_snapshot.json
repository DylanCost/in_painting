{
  "common": {
    "unet": {
      "input_channels": 3,
      "hidden_dims": [
        64,
        128,
        256,
        512,
        512
      ],
      "use_attention": true,
      "use_skip_connections": true,
      "dropout": 0.1
    },
    "data": {
      "dataset": "celeba",
      "data_path": "./assets/datasets",
      "image_size": 128,
      "num_workers": 4,
      "augmentation": true,
      "batch_size": 64,
      "learning_rate": 0.0002,
      "epochs": 100
    },
    "mask": {
      "type": "random",
      "mask_ratio": 0.4,
      "min_size": 16,
      "max_size": 64,
      "seed": 42,
      "cache_dir": "./assets/masks"
    },
    "logging": {
      "use_wandb": true,
      "use_tensorboard": true,
      "log_interval": 100,
      "save_interval": 5,
      "sample_interval": 500,
      "checkpoint_dir": "./weights",
      "log_dir": "./logs"
    }
  },
  "flowmatching": {
    "epochs": 100,
    "batch_size": 64,
    "learning_rate": 0.0001,
    "weight_decay": 0.0,
    "warmup_steps": 1000,
    "min_lr": 1e-05,
    "subsample_fraction": 1.0,
    "num_sampling_steps": 100,
    "num_eval_samples": 1024,
    "num_example_images": 8,
    "compute_lpips": false,
    "device": "cuda",
    "seed": 42
  }
}